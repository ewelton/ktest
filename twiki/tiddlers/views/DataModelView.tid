\define displaysubtype(elementtype)
<$set name="elementtype" value="$elementtype$">
<$list filter="[[SubTypeFields]getindex<elementtype>]" variable="field">
<$list filter="[[SubTypeLabels]getindex<elementtype>]" variable="label">
<$list filter="[all[current]get<field>]" variable="value">
<h3><<label>>:<<value>></h3>
</$list>
</$list>
</$list>
</$set>
\end


<$macrocall $name="displaysubtype" elementtype={{!!element.type}} />


<$setvars
_etype = {{!!element.type}}
_prefix = "$:/twiki/"
_subtype = {{!!org.type}}
tname="prefix etype \/Viewer/\subtype"
>
<$transclude tiddler=<<tname>> />
</$setvars>
